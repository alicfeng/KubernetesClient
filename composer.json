{
  "name": "alicfeng/kubernetes-client",
  "description": "Kubernetes PHP Client",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "kubernetes",
    "k8s",
    "asm",
    "php",
    "alicfeng",
    "samego"
  ],
  "authors": [
    {
      "name": "AlicFeng",
      "email": "a@samego.com",
      "homepage": "https://www.samego.com",
      "role": "Software Developer"
    },
    {
      "name": "lsrong",
      "homepage": "https://github.com/lsrong",
      "role": "Web Developer"
    },
    {
      "name": "lljiuzheyang",
      "homepage": "https://github.com/lljiuzheyang",
      "role": "Web Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "AlicFeng\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "composer/composer": "^1.8",
    "friendsofphp/php-cs-fixer": "^2.18",
    "orchestra/testbench": "^3.5"
  },
  "minimum-stability": "stable",
  "require": {
    "guzzlehttp/guzzle": "7.2.0",
    "php": "^7.1|^8.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-gd": "*",
    "ext-curl": "*",
    "symfony/yaml": "^5.2"
  },
  "scripts": {
    "php-cs": [
      "./vendor/bin/php-cs-fixer fix src",
      "./vendor/bin/php-cs-fixer fix tests"
    ]
  },
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
